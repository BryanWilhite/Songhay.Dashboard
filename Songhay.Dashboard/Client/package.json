    "@songhay/core": "0.0.2",
    "@songhay/player-video-you-tube": "0.0.4",
    "aspnet-prerendering": "^3.0.1",
    "core-js": "^2.6.11",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "rxjs": "~6.3.3",
