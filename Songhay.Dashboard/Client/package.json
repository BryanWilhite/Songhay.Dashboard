{
  "name": "client",
  "version": "0.0.0",
  "scripts": {
    "index:cleancss": "npx cleancss -o ./src/assets/css/index.min.css ./src/assets/css/index-bundle.css",
    "index:concat": "concat -o ./src/assets/css/index-bundle.css ./node_modules/@material/dialog/dist/mdc.dialog.css ./node_modules/@material/typography/dist/mdc.typography.css ./src/assets/css/index.css",
    "index:sass": "sass ./src/assets/scss/index.scss ./src/assets/css/index.css",
    "ng": "ng",
    "start": "ng serve --open=true",
    "prebuild": "npm run index:sass && npm run index:concat && npm run index:cleancss",
    "build": "ng build",
    "prebuild:prod": "npm run index:sass && npm run index:concat && npm run index:cleancss",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^0.901.12",
