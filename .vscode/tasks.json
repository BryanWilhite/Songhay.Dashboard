{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Songhay.Dashboard.sln"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "type": "npm",
            "script": "build",
            "path": "Songhay.Dashboard/ClientApp/",
            "problemMatcher": ["$tslint5"]
        },
        {
            "type": "npm",
            "script": "start",
            "path": "Songhay.Dashboard/ClientApp/",
            "problemMatcher": ["$tslint5"]
        }
    ]
}